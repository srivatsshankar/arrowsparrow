{
  "expo": {
    "name": "Arrow Sparrow",
    "slug": "arrow-sparrow-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "arrowsparrow",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-document-picker",
      "expo-av",
      "expo-audio",
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
